{
  "name": "viewtube",
  "description": "ViewTube, an alternative Youtube frontend.",
  "author": "Maurice Oegerli",
  "country": "Switzerland",
  "email": "viewtube@m-o.dev",
  "version": "0.7.0",
  "license": "AGPLv3",
  "private": true,
  "scripts": {
    "start": "node -r module-alias/register dist/server/main.js",
    "build": "rimraf dist && nuxt build && nest build server --path tsconfig.build.json --config nest-cli.json",
    "serve": "cross-env NODE_ENV=development webpack",
    "serve:api": "cross-env NODE_ENV=development API_ONLY=true PORT=8067 nest start",
    "serve:web": "cross-env NODE_ENV=development VIEWTUBE_API_URL=\"http://localhost:8067/api/\" nuxt dev",
    "test": "jest",
    "e2e": "supertest",
    "start:local": "cross-env NODE_ENV=production node -r module-alias/register dist/server/main.js",
    "build:local": "cross-env NODE_ENV=production npm run build",
    "db": "mongod --dbpath data",
    "redis": "redis-server redis.conf",
    "prettier": "prettier --write .",
    "lint": "eslint --ext .ts,.js,.vue .",
    "lintfix": "eslint --ext .ts,.js,.vue --fix .",
    "clean": "rimraf dist",
    "nest": "nest",
    "nuxt": "nuxt",
    "postinstall": "node ./prepare.js",
    "lint-staged": "prettier --write --ignore-unknown"
  },
  "_moduleAliases": {
    "server": "dist/server"
  },
  "browserslist": [
    "last 1 version",
    "not IE 11",
    "not IE_Mob 11",
    "not Edge 18"
  ],
