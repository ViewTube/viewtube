    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "hex-rgb": "5.0.0",
    "openapi-typescript": "6.7.4",
    "redis": "4.6.13",
    "rimraf": "5.0.5",
    "run-script-webpack-plugin": "0.2.0",
    "ts-loader": "9.5.1",
    "typescript": "5.3.3",
    "webpack": "5.90.3",
    "webpack-node-externals": "3.0.0"
  }
