{
  "name": "@viewtube/server",
  "main": "dist/main",
  "engines": {
    "node": ">=20.11.0",
    "pnpm": ">=9.1.3"
  },
  "scripts": {
    "serve": "pnpm -w run build:shared && nest start -w",
    "dev": "pnpm run serve",
    "build": "cross-env NODE_ENV=production nest build",
    "start": "cross-env NODE_ENV=production node ./dist/main.js",
    "gen:swagger": "pnpm run build && cross-env GENERATE_SWAGGER=true node ./dist/main.js",
    "gen:api": "pnpm run gen:swagger && openapi-typescript swagger-spec.json --output ../shared/src/api.schema.ts && rimraf swagger-spec.json"
  },
  "_moduleAliases": {
    "server": "dist/server"
  },
  "dependencies": {
    "@fastify/cookie": "9.3.1",
    "@fastify/helmet": "11.1.1",
    "@fastify/multipart": "8.2.0",
    "@fastify/static": "7.0.4",
    "@fastify/swagger": "8.14.0",
    "@nestjs/bull": "10.1.1",
    "@nestjs/cache-manager": "2.2.2",
    "@nestjs/common": "10.3.8",
    "@nestjs/config": "3.2.2",
    "@nestjs/core": "10.3.8",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/mongoose": "10.0.6",
    "@nestjs/platform-fastify": "10.3.8",
    "@nestjs/schedule": "4.0.2",
    "@nestjs/swagger": "7.3.1",
    "@viewtube/shared": "workspace:*",
    "archiver": "7.0.1",
    "axios": "1.7.2",
    "bcryptjs": "2.4.3",
    "bull": "4.12.9",
    "cache-manager": "5.5.3",
    "cache-manager-redis-yet": "5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cookie-parser": "1.4.6",
    "dayjs": "1.11.11",
    "destr": "2.0.3",
    "fast-xml-parser": "4.4.0",
    "fastify": "4.27.0",
    "fetch-socks": "1.3.0",
    "https-proxy-agent": "7.0.4",
    "humanize-duration": "3.32.1",
    "ioredis": "5.4.1",
    "joi": "17.13.1",
    "mini-svg-data-uri": "1.4.4",
    "mongoose": "8.4.0",
    "nest-winston": "1.10.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "sharp": "0.33.4",
    "svg-captcha": "1.4.0",
    "tslib": "2.6.2",
    "undici": "6.18.1",
    "web-push": "3.6.7",
    "winston": "3.13.0",
    "winston-daily-rotate-file": "5.0.0",
    "xml-js": "1.6.11",
    "youtubei.js": "9.4.0",
    "ytpl": "2.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.2",
    "@nestjs/websockets": "10.3.8",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.7",
    "@types/archiver": "6.0.2",
    "@types/bcryptjs": "2.4.6",
    "@types/cookie-parser": "1.4.7",
    "@types/humanize-duration": "3.27.4",
    "@types/mime-types": "2.1.4",
    "@types/multer": "1.4.11",
