{
  "name": "server",
  "main": "dist/main",
  "engines": {
    "node": ">=18.16.0",
    "pnpm": ">=8.7.0"
  },
  "scripts": {
    "serve": "nest build --webpack --webpackPath webpack-hmr.config.cjs --watch",
    "dev": "pnpm run serve",
    "build": "cross-env NODE_ENV=production nest build -c nest-cli.json --webpack --webpackPath webpack.config.cjs",
    "start": "cross-env NODE_ENV=production node ./dist/main.cjs",
    "lint": "eslint --ext .ts,.js .",
    "lint:fix": "eslint --fix .",
    "gen:swagger": "pnpm run build && cross-env GENERATE_SWAGGER=true node ./dist/main.cjs",
    "gen:api": "pnpm run gen:swagger && openapi-typescript swagger-spec.json --output ../shared/api.schema.ts && rimraf swagger-spec.json"
  },
  "_moduleAliases": {
    "server": "dist/server"
  },
  "dependencies": {
    "@fastify/cookie": "9.3.1",
    "@fastify/helmet": "11.1.1",
    "@fastify/multipart": "8.1.0",
    "@fastify/static": "7.0.1",
    "@fastify/swagger": "8.14.0",
    "@freetube/yt-comment-scraper": "6.2.0",
    "@nestjs/bull": "10.1.0",
    "@nestjs/cache-manager": "2.2.1",
    "@nestjs/common": "10.3.3",
    "@nestjs/config": "3.2.0",
    "@nestjs/core": "10.3.3",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/mongoose": "10.0.4",
    "@nestjs/platform-fastify": "10.3.3",
    "@nestjs/schedule": "4.0.1",
    "@nestjs/swagger": "7.3.0",
    "archiver": "6.0.2",
    "bcryptjs": "2.4.3",
    "bull": "4.12.2",
    "cache-manager": "5.4.0",
    "cache-manager-redis-yet": "4.1.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14",
    "cookie-parser": "1.4.6",
    "dayjs": "1.11.10",
    "destr": "2.0.3",
    "fast-xml-parser": "4.3.5",
    "fastify": "4.26.1",
    "fetch-socks": "1.2.0",
    "https-proxy-agent": "7.0.4",
    "humanize-duration": "3.31.0",
    "ioredis": "5.3.2",
    "joi": "17.12.2",
    "mini-svg-data-uri": "1.4.4",
    "mongoose": "7.6.9",
    "nest-winston": "1.9.4",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "sharp": "0.33.2",
    "svg-captcha": "1.4.0",
    "tslib": "2.6.2",
    "undici": "6.6.2",
    "web-push": "3.6.7",
