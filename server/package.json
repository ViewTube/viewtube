    "winston-daily-rotate-file": "5.0.0",
    "xml-js": "1.6.11",
    "youtubei.js": "9.1.0",
    "ytpl": "2.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.2",
    "@nestjs/websockets": "10.3.3",
    "@types/archiver": "6.0.2",
    "@types/bcryptjs": "2.4.6",
    "@types/cookie-parser": "1.4.6",
    "@types/eslint": "8.56.3",
    "@types/humanize-duration": "3.27.4",
