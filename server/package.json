    "ytpl": "2.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.2.1",
    "@nestjs/websockets": "10.2.6",
    "@types/archiver": "5.3.4",
    "@types/bcryptjs": "2.4.5",
    "@types/cookie-parser": "1.4.5",
    "@types/eslint": "8.44.6",
    "@types/humanize-duration": "3.27.2",
